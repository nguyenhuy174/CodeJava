package bh10;

public class timKiem extends Thread {
	private int soCanTim;
	private int a;
	private int n;

	public timKiem(int soCanTim, int a, int n) {
		this.soCanTim = soCanTim;
		this.a = a;
		this.n = n;
	}

	@Override
	public void run() {
		int a[] = new int[n];
		for (var i = 1; i <= n; i++) {
			a[i] = i;
		}
		
	}
}
